<!DOCTYPE html>
<html>
  <head>
    <title>ECommercePlatform</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <%= render 'layouts/header' %>
    <%= yield %>
    
    <%= render 'layouts/footer' %>

    <%= javascript_include_tag 'js/jquery-3.2.1.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'js/popper.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'js/bootstrap.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'js/stellar.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'vendors/lightbox/simpleLightbox.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'vendors/nice-select/js/jquery.nice-select.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'vendors/isotope/imagesloaded.pkgd.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'vendors/isotope/isotope-min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'vendors/owl-carousel/owl.carousel.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'js/jquery.ajaxchimp.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'vendors/counter-up/jquery.waypoints.min.js', 'data-turbolinks-track': 'reload' %>

    <%= javascript_include_tag 'vendors/counter-up/jquery.counterup.js', 'data-turbolinks-track': 'reload' %>

    <%= javascript_include_tag 'js/mail-script.js', 'data-turbolinks-track': 'reload' %>

    <%= javascript_include_tag 'js/theme.js', 'data-turbolinks-track': 'reload' %>


   
   
  
    
  </body>
</html>
