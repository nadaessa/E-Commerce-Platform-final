<p id="notice"><%= notice %></p>

<h1>Coupones</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Value</th>
      <th>Expiration type</th>
      <th>Time</th>
      <th>No of usage</th>
      <th>Code</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coupones.each do |coupone| %>
      <tr>
        <td><%= coupone.type %></td>
        <td><%= coupone.value %></td>
        <td><%= coupone.expiration_type %></td>
        <td><%= coupone.time %></td>
        <td><%= coupone.no_of_usage %></td>
        <td><%= coupone.code %></td>
        <td><%= coupone.status %></td>
        <td><%= link_to 'Show', coupone %></td>
        <td><%= link_to 'Edit', edit_coupone_path(coupone) %></td>
        <td><%= link_to 'Destroy', coupone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coupone', new_coupone_path %>
